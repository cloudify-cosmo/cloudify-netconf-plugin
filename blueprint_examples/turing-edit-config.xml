<?xml version='1.0' encoding='UTF-8'?>
<rfc6020:rpc xmlns:rfc6020="urn:ietf:params:xml:ns:netconf:base:1.0" xmlns:t2="http://example.net/turing-machine/tape-2" xmlns="http://example.net/turing-machine" message-id="11">
  <edit-config>
    <config>
      <turing-machine>
        <transition-function>
          <delta>
            <input>
              <state>0</state>
              <symbol>0</symbol>
            </input>
            <label>1</label>
            <output>
              <symbol/>
              <head-move>right</head-move>
              <state>1</state>
            </output>
          </delta>
          <delta>
            <input>
              <state>0</state>
              <symbol>1</symbol>
            </input>
            <output>
              <symbol>1</symbol>
              <head-move>left</head-move>
              <state>1</state>
            </output>
            <label>2</label>
          </delta>
        </transition-function>
      </turing-machine>
    </config>
    <target>running</target>
  </edit-config>
</rfc6020:rpc>
